generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Word {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  meaning   String
  fixed     Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  book      String?
}
